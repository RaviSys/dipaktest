<table class="container">
  <tr>
    <th>S.No</th>
    <th>Pri</th>
    <th>Question</th>
    <th>Teaming Stages</th>
    <th>Appears (Day)</th>
    <th>Frequency</th> 
    <th>Type</th>
    <th>Role</th>
    <th>Required</th>
    <th>Conditions</th>
    <th>Mapping</th>
    <th>Action</th>
  </tr> 
  <%@users.each do |user|%>   
  <tr>
    <td><%=user.id%></td>
    <td><%=user.file['Pri']%></td>
    <td><%=user.file['Question']%></td>
    <td><%=user.file['Teaming Stages']%></td>
    <td><%=user.file['Appears (Day)']%></td>
    <td><%=user.file['Frequency']%></td>
    <td><%=user.file['Type']%></td>
    <td><%=user.file['Role']%></td>
    <td><%=user.file['Required?']%></td>
    <td><%=user.file['Conditions']%></td>
    <td><%=user.file['Mapping']%></td>
    <td>
      <%= link_to 'Destroy', user, method: :delete, data: { confirm:
        'Are you sure?' }, :remote => true, :class => 'delete_pony' %>
        <%=link_to 'Edit',edit_user_url(user)%>
      </td>    
    </tr>
    <%end%>
  </table><br>
  <%= will_paginate @users %>


